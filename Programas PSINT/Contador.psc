Proceso Contador
	definir contador Como Entero;
	para contador<-1 Hasta 10 Con Paso 1 Hacer
		escribir contador;
	FinPara
	contador<-1;
	Mientras contador <= 10 Hacer
		Escribir contador;
		contador <- contador + 1;
	FinMientras
	contador<-1;
	Repetir
		Escribir contador;
		contador <- contador + 1;
	Hasta Que contador >= 10
FinProceso
